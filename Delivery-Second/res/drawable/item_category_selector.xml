<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:drawable="@drawable/bg_category_item_normal" />
    <item android:state_enabled="false" android:drawable="@drawable/bg_category_item_pressed" />
    <item android:state_focused="true" android:state_pressed="false" android:drawable="@drawable/bg_category_item_normal" />
